function fancyprint(text, y, clr)
 width = (#text * 10) + (#text - 1)
 startx = (58 - (width/2))
 for i=1,#text do
  x = startx + ((i-1) * 5) + (i*7)
  bigletter(sub(text,i,i), x, y, clr)
 end
end

function bigletter(char, x, y, clr)
 l = bigletters[char]
 for i=1,#l do
  line(x+l[i][1],y+l[i][2],x+l[i][3],y+l[i][4], clr)
 end
end

function center(text, clr, y)
 if (not y) y = 62
 x=64 - ((#text * 3) + (#text - 1)) / 2
 print(text, x, y, clr)
end

