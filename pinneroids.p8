 i=flr(rnd(4))
 if (i == 0) then
  covid = {
   x = rnd(64) + 32,
   y = 0,
   dir = 270,
  }
 end

 if (i == 1) then
  covid = {
   x = rnd(64) + 32,
   y = 128,
   dir = 90,
  }
 end

 if (i == 2) then
  covid = {
   y = rnd(64) + 32,
   x = 0,
   dir = 0,
  }
 end

 if (i == 3) then
  covid = {
   y = rnd(64) + 32,
   x = 128,
   dir = 180,
  }
 end
 
 covidtimer = null
 covid.targetdir = covid.dir
 covid.turncounter = 0
 covid.w = 3
 covid.t = 0

 -- fake a shot to pause
 covidshot = {}
 covidshot.t=30
 covidshot.active = false

end
